

    <!-- BEGIN: Content-->
<section id="data-list-view" class="data-list-view-header">
  @if (ViewBag.er != null)
{
<p class="alert alert-success">@ViewBag.er</p>
}
  <!-- DataTable starts -->
  <div class="table-responsive">
    <table class="table data-list-view">
      <thead>
        <tr>
          <th></th>
          <th>ردیف</th>
          <th> نام کارشناس </th>
          <th> کد کارشناس </th>
          <th> تصویر کارشناس </th>
          <th> آدرس </th>
           <th>  زبان</th>
          <th> عملیات</th>
        </tr>
      </thead>
      <tbody>
@if (ViewBag.list != null)
{
  int i=1;
     foreach (var item in ViewBag.list)
     {
           <tr>
          <td></td>
          <td class="product-category"> @i</td>
          <td class="product-name">@item.NameEx</td>
            <td class="product-category">@item.Id </td>

           <td class="product-category">  <img style="width: 60px;" src="~/fileupload/@item.ImageEx" alt="">  </td>
            <td class="product-category">@item.address </td>
                        <td class="product-category">@item.Language </td>
           <td class="product-action">
             <div class="col-lg-3 col-md-3 mb-1">
                <div class="btn-group" role="group" aria-label="Basic example">
                 
                  <a href="/admin/experts/del/@item.Id" type="button" class="btn btn-danger btn-sm" onclick="return confirm('آیا از حذف رکورد مورد نظر مطمن هستید؟')">حذف</a>
                 
                </div>
              </div>
          </td>
        </tr>
 i++;
     }
}
      

      


      </tbody>
    </table>
  </div>
  <!-- DataTable ends -->

  <!-- add new sidebar starts -->
 
  <!-- add new sidebar ends -->
</section>
<!-- Data list view end -->
